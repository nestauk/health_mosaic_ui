{
  "name": "health-innovation-scanner",
  "description": "Mapping health innovation globally for the Robert Wood Johnson Foundation.",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run lambda:build && sapper build --legacy",
    "export": "npm run lambda:build && rollup -c rollup-lambda.config.js && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "jest",
    "cleansrc": "rimraf src/node_modules",
    "cleanall": "rimraf node_modules src/node_modules __sapper__ package-lock.json",
    "reinstall:npm": "npm run cleanall && npm i",
    "reinstall:yarn": "yarn cleanall && yarn",
    "lambda:dev": "NODE_ENV=development netlify-lambda serve dist/lambda",
    "lambda:build": "rollup -c rollup-lambda.config.js && netlify-lambda build dist/lambda"
  },
  "dependencies": {
    "@svizzle/utils": "^0.2.0",
    "apollo-boost": "^0.4.1",
    "apollo-datasource-rest": "^0.3.2",
    "apollo-server-lambda": "^2.4.8",
    "compression": "^1.7.1",
    "encoding": "^0.1.12",
    "graphql": "^14.2.1",
    "ionicons": "^4.5.5",
    "just-compare": "^1.3.0",
    "lamb": "^0.57.0",
    "mapbox-gl": "^0.53.1",
    "netlify-lambda": "^1.4.5",
    "polka": "^0.4.0",
    "sirv": "^0.2.5",
    "xstate": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "chokidar": "^2.1.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsdoc": "^4.8.3",
    "eslint-plugin-svelte3": "https://github.com/sveltejs/eslint-plugin-svelte3.git",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.15.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy-files": "^0.1.1",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "sapper": "^0.27.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.2.0",
    "svelte": "^3.5.1",
    "ts-jest": "^23.10.5",
    "ts-lib": "^0.0.5",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  }
}
