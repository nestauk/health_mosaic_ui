{
  "name": "health-innovation-scanner",
  "description": "Mapping health innovation globally for the Robert Wood Johnson Foundation.",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "npm run lambda:build && sapper build --legacy",
    "export": "npm run lambda:build && rollup -c rollup-lambda.config.js && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "cleansrc": "rimraf src/node_modules",
    "cleanall": "rimraf node_modules src/node_modules __sapper__ package-lock.json",
    "data:geo:world": "mkdirp data/geo && node -r esm bin/world_atlas_ids.js",
    "data:geo:lookup": "mkdirp data/geo && node -r esm bin/lookup.js",
    "reinstall:npm": "npm run cleanall && npm i",
    "reinstall:yarn": "yarn cleanall && yarn",
    "lambda:dev": "NODE_ENV=development netlify-lambda serve dist/lambda",
    "lambda:build": "rollup -c rollup-lambda.config.js && netlify-lambda build dist/lambda",
    "copy:screen": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node bin/screen_machine.ts",
    "copy:search": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node bin/search_machine.ts"
  },
  "dependencies": {
    "@svizzle/dom": "^0.2.0",
    "@svizzle/geo": "^0.2.0",
    "@svizzle/utils": "^0.2.0",
    "apollo-boost": "^0.4.3",
    "apollo-datasource-rest": "^0.5.0",
    "apollo-server-lambda": "^2.6.7",
    "compression": "^1.7.1",
    "d3-array": "^2.2.0",
    "d3-format": "^1.3.2",
    "d3-geo": "^1.11.3",
    "d3-geo-projection": "^2.7.0",
    "d3-scale": "^3.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "encoding": "^0.1.12",
    "graphql": "^14.4.2",
    "ionicons": "^4.6.1",
    "just-compare": "^1.3.0",
    "lamb": "^0.58.0",
    "mapbox-gl": "^1.1.0",
    "netlify-lambda": "^1.4.13",
    "polka": "^1.0.0-next.4",
    "sirv": "^0.4.2",
    "topojson-client": "^3.0.0",
    "world-atlas": "^1.1.4",
    "xstate": "^4.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@svizzle/file": "^0.3.0",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "chokidar": "^3.0.1",
    "clipboardy": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsdoc": "^10.2.0",
    "eslint-plugin-svelte3": "^2.5.0",
    "eslint-plugin-typescript": "^0.14.0",
    "esm": "^3.2.25",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "node-mkdirp": "0.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy-files": "^0.1.1",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sapper": "^0.27.4",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "svelte": "^3.6.4",
    "ts-jest": "^24.0.2",
    "ts-lib": "^0.0.5",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
